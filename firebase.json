{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
		"source": "/datum",
		"destination": "/datum.html"
	  },
	  {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
